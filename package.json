{
  "name": "speedcubertimer",
  "version": "0.2.4",
  "private": true,
  "scripts": {
    "env": "dotenv -e ./fastlane/.env",
    "start": "yarn env react-native start",
    "ios": "yarn env react-native run-ios",
    "android": "yarn env react-native run-android",
    "ios:release": "yarn env react-native run-ios --mode Release",
    "android:release": "yarn env react-native run-android --mode release",
    "build:ios": "yarn env yarn pods && react-native bundle --platform ios",
    "build:android": "yarn env react-native bundle --platform android",
    "pods": "npx pod-install",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "clean": "npx react-native-clean-project",
    "analyze-bundle": "npx react-native-bundle-visualizer - only-mapped - bundle-output ./myapp.bundle - format html"
  },
