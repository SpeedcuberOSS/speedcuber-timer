{
  "name": "speedcubertimer",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native rnx-start",
    "start-no-cache": "react-native rnx-start --reset-cache",
    "env:storybook": "node envs/set-environment.js storybook && rnstl",
    "env:dev": "node envs/set-environment.js dev",
    "env:qa": "node envs/set-environment.js qa",
    "env:prod": "node envs/set-environment.js prod",
    "_android": "react-native run-android",
    "_ios": "react-native run-ios",
    "ios": "yarn env:dev && yarn _ios",
    "ios:qa": "yarn env:qa && yarn _ios",
    "ios:prod": "yarn env:prod && yarn _ios --configuration Release",
    "ios:storybook": "yarn env:storybook && yarn _ios",
    "android": "yarn env:dev && yarn _android",
    "android:qa": "yarn env:qa && yarn _android",
    "android:prod": "yarn env:prod && yarn _android --variant release",
    "android:storybook": "yarn env:storybook && yarn _android",
    "_build:ios": "yarn pods && react-native rnx-bundle --platform ios && yarn fix-dependencies",
    "build:ios": "yarn env:dev && yarn _build:ios",
    "build:ios:qa": "yarn env:qa && yarn _build:ios",
    "build:ios:prod": "yarn env:prod && yarn _build:ios",
    "build:ios:storybook": "yarn env:storybook && yarn _build:ios",
    "_build:android": "react-native rnx-bundle --platform android && yarn fix-dependencies",
    "build:android": "yarn env:dev && yarn _build:android",
    "build:android:qa": "yarn env:qa && yarn _build:android",
    "build:android:prod": "yarn env:prod && yarn _build:android",
    "build:android:storybook": "yarn env:storybook && yarn _build:android",
    "pods": "cd ios && pod install",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "clean": "react-native-clean-project",
    "link-assets": "react-native-asset",
    "storybook": "start-storybook -p 7007",
    "build-storybook": "build-storybook",
    "prestorybook": "rnstl",
    "stories": "rnstl",
    "analyze-bundle": "react-native-bundle-visualizer - only-mapped - bundle-output ./myapp.bundle - format html",
    "check-dependencies": "rnx-align-deps",
    "fix-dependencies": "rnx-align-deps --write",
    "deduplicate-dependencies": "yarn-deduplicate yarn.lock"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.3",
    "@react-native-community/slider": "^4.4.1",
    "@react-native-masked-view/masked-view": "^0.2.6",
    "@react-navigation/bottom-tabs": "^6.5.4",
    "@react-navigation/drawer": "^6.5.8",
    "@react-navigation/material-bottom-tabs": "^6.2.12",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/stack": "^6.2.0",
    "@types/react": "^17",
    "buffer": "^6.0.3",
    "cubing": "^0.31.7",
    "deepmerge": "^3.3.0",
    "i18next": "^21.8.9",
    "intl-pluralrules": "^1.3.1",
    "lodash": "^4.17.21",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hooks-global-state": "^2.0.0",
    "react-i18next": "^11.17.0",
    "react-native": "^0.68.0",
    "react-native-ble-manager": "^8.4.4",
    "react-native-ble-plx": "^2.0.3",
    "react-native-charts-wrapper": "^0.5.11",
    "react-native-default-preference": "^1.4.4",
    "react-native-flipper": "^0.176.1",
    "react-native-fs": "^2.18.0",
    "react-native-gesture-handler": "^2.3.2",
    "react-native-get-random-values": "^1.8.0",
    "react-native-localize": "^2.2.2",
    "react-native-paper": "^5.1.4",
    "react-native-reanimated": "^2.5.0",
    "react-native-reanimated-carousel": "^3.3.0",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^3.13.1",
    "react-native-svg": "^12.3.0",
    "react-native-vector-icons": "^9.1.0",
    "react-native-webview": "^11.22.6",
    "rn-flipper-async-storage-advanced": "^1.0.4",
    "solution-analyzer": "^0.1.6",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/cli-platform-android": "^7.0.1",
    "@react-native-community/eslint-config": "^2.0.0",
    "@rnx-kit/align-deps": "^2.1.3",
    "@rnx-kit/babel-preset-metro-react-native": "^1.1.4",
    "@rnx-kit/cli": "^0.16.2",
    "@rnx-kit/metro-config": "^1.3.5",
    "@rnx-kit/metro-resolver-symlinks": "^0.1.25",
    "@storybook/addon-actions": "^5.3",
    "@storybook/addon-knobs": "^5.3",
    "@storybook/addon-links": "^5.3",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.26",
    "@storybook/addon-storyshots": "^5.3.21",
    "@storybook/react-native": "^5.3.25",
    "@storybook/react-native-server": "^5.3.23",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.191",
    "@types/react-native": "^0.67.3",
    "@types/react-native-charts-wrapper": "^0.5.2",
    "@types/react-native-vector-icons": "^6.4.10",
    "@types/react-test-renderer": "^17.0.1",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.5",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.67.0",
    "react-native-asset": "^2.0.1",
    "react-native-bundle-visualizer": "^3.1.2",
    "react-native-clean-project": "^4.0.1",
    "react-native-storybook-loader": "^2.0.5",
    "react-native-svg-app-icon": "^0.6.1",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.4.4",
    "yarn-deduplicate": "^6.0.1"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?@react-native|react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/__mocks__/globalMock.js"
    ]
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src",
        "./packages",
        "./storybook"
      ],
      "pattern": "**/*.stories.tsx",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "rnx-kit": {
    "kitType": "app",
    "bundle": {
      "treeShake": true,
      "plugins": [
        "@rnx-kit/metro-plugin-cyclic-dependencies-detector",
        [
          "@rnx-kit/metro-plugin-duplicates-checker",
          {
            "ignoredModules": [
              "color-convert",
              "color-name",
              "@react-native/normalize-color",
              "escape-string-regexp",
              "color",
              "react-is"
            ]
          }
        ],
        "@rnx-kit/metro-plugin-typescript"
      ]
    },
    "alignDeps": {
      "requirements": [
        "react-native@0.68"
      ],
      "capabilities": [
        "animation",
        "babel-preset-react-native",
        "community/cli-android",
        "core",
        "core-android",
        "core-ios",
        "filesystem",
        "gestures",
        "jest",
        "masked-view",
        "navigation/native",
        "navigation/stack",
        "react",
        "react-dom",
        "react-test-renderer",
        "safe-area",
        "screens",
        "storage",
        "svg",
        "webview"
      ]
    }
  }
}